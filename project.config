[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/DennySPb (DennySPB)
[access "refs/for/refs/*"]
	read = group user/DennySPb (DennySPB)
	push = group user/DennySPb (DennySPB)
	label-Code-Review = -2..+2 group user/DennySPb (DennySPB)
	label-Verified = -1..+1 group user/DennySPb (DennySPB)
	rebase = group user/DennySPb (DennySPB)
	removeReviewer = group user/DennySPb (DennySPB)
	submit = group user/DennySPb (DennySPB)
[access "refs/heads/*"]
	read = group user/DennySPb (DennySPB)
	create = group user/DennySPb (DennySPB)
	push = +force group user/DennySPb (DennySPB)
	pushMerge = group user/DennySPb (DennySPB)
[access "refs/tags/*"]
	push = group user/DennySPb (DennySPB)
	push = +force group user/DennySPb (DennySPB)
[submit]
	action = rebase if necessary
